slova = {  "КРИНЖ": "Что-то стыдное или странное"
           "ЛОЛ": "Что-то очень смешное"
            РОФЛ": "Прикол, шутка"
        }
        
word = input("Введите непонятное слово (большими буквами!): ")     
if word in slova.keys():
    print(slova[word])
else:   
    print("Слово", word, "Не нашлось")
